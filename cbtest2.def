Component Builder 2.0
COMPONENT-BUILDER-OPTIONS:
  COMPONENT-MODE:        CTL
  AUTO-UPDATE-FILE-DEFN: true
  SHOW-NODE-NAMES:       false
  DEFAULT-FONT-SIZE:     10

PARAMETERS:
  SECTION: "CONFIGURATION"
    ctrlGrp "Assigned Control Group" "" 0 INTEGER 1 1 54
    Pri     "Priority Level"         "" 0 INTEGER 1 1
  SECTION: "ParameterSelection"
    numInputs  "Description" ""                     0 INTEGER 1 1 4
    type       "Description" "ANN;LSTM;RNN"         0 TOGGLE  0
    numLayers  "Description" ""                     0 INTEGER 1 1 3
    numNodes   "Description" ""                     0 INTEGER 1 1 5  type == LSTM
    numWeights "Description" ""                     0 INTEGER 5 1 12 type==ANN
    timesteps  "Description" ""                     0 INTEGER 5 1 20
    activation "Description" "LINEAR;TANH;RELU;SIG" 0 TOGGLE  1
    batch_size "Description" ""                     0 INTEGER 1

GRAPHICS:
  Box(-32,-160,56,160)
  Line(104,144,104,144)
  Line(56,-8,56,-8)
  Line(56,-8,56,-8)
  Line(56,-8,56,-8)
  Line(56,-8,56,-8)
  Line(-32,-32,-88,-32)
  If (numInputs > 1)
    Line(-32,-64,-88,-64)
  EndIf
  If (numInputs > 2)
    Line(-32,-96,-88,-96)
  EndIf
  If (numInputs>3)
    Line(-32,-128,-88,-128)
  EndIf
  Line(-32,0,-96,0)
  Line(-32,32,-96,32)
  Line(56,0,128,0)
  FontSize(10)
  FText(-104,48,"Learning Rate")
  FontSize(10)
  FText(-104,16,"Target")
  FontSize(10)
  FText(-104,-16,"Input1")
  FontSize(10)
  FText(-96,-48,"Input 2")
  FontSize(10)
  FText(-96,-80,"Input3")
  FontSize(10)
  FText(-96,-112,"input4")

NODES:
  INITIALIZE_NODE input1        = _dud_
  INITIALIZE_NODE input2        = _dud_
  INITIALIZE_NODE input3        = _dud_
  INITIALIZE_NODE input4        = _dud_
  INITIALIZE_NODE foutput       = _dud_
  INITIALIZE_NODE label         = _dud_
  INITIALIZE_NODE Learning_rate = _dud_

  input1 -3 -1 INPUT REAL
  #IF numInputs>1
    input2 -3 -2 INPUT REAL
    #IF numInputs>2
      input3 -3 -3 INPUT REAL
      #IF numInputs>3
        input4 -3 -4 INPUT REAL
      #END
    #END
  #END
  foutput 4 0 OUTPUT REAL
  label -3 0 INPUT REAL
  Learning_rate -3 1 INPUT REAL

VALVE-GROUP:
  #IF  type==ANN
    RTDS.CTL.RISC_CMODEL
        V4COMP: ModelName = ANN SubSystem = $SS Processor = $ctrlGrp Priority = $Pri
        DATA_SIZES: INTDATA=5 FLOATDATA=1 CHARDATA_FRMT=0 INVARS=6 OUTVARS=1
        INTDATA: $numInputs numInputs $type type $numLayers numLayers $numWeights numWeights $activation activation
        FLOATDATA: $batch_size batch_size
        INVARS: $input1 IEEE input1 $label IEEE label $Learning_rate IEEE Lr $input2 IEEE input2 $input3 IEEE input3
        INVARS: $input4 IEEE input4
        OUTVARS: $foutput IEEE foutput
    END

  #ELSEIF  type==LSTM
    RTDS.CTL.RISC_CMODEL
        V4COMP: ModelName = LSTMpart SubSystem = $SS Processor = $ctrlGrp Priority = $Pri
        DATA_SIZES: INTDATA=5 FLOATDATA=0 CHARDATA_FRMT=0 INVARS=6 OUTVARS=1
        INTDATA: $numInputs numInputs $type type $numLayers numLayers $numNodes numNodes $numWeights numWeights
        INVARS: $input1 IEEE input1 $input2 IEEE input2 $input3 IEEE input3 $input4 IEEE input4 $label IEEE label
        INVARS: $Learning_rate IEEE Learning_rate
        OUTVARS: $foutput IEEE foutput
    END

  #ELSE 
    RTDS.CTL.RISC_CMODEL
        V4COMP: ModelName = rnn SubSystem = $SS Processor = $ctrlGrp Priority = $Pri
        DATA_SIZES: INTDATA=4 FLOATDATA=0 CHARDATA_FRMT=0 INVARS=3 OUTVARS=1
        INTDATA: $numInputs numInputs $type type $numLayers numLayers $numNodes numNodes
        INVARS: $$input1 INT input1 $label IEEE label $Learning_rate IEEE Lr
        OUTVARS: $foutput IEEE foutput
    END

  #END 
COMPONENT-DESCRIPTORS:
  CLASSIFICATION: GENERAL
  PHASES:         N/A
  MAINSTEP:       YES
  SUBSTEP:        NO
  SUPERSTEP:      NO
  SMALLDT:        NO
  GPES:           NO
  TWRT:           NO
  DIST:           NO
  DEPRECATED:     NO

